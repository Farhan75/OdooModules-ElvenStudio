<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- ************* file_line Tree View  *************** -->
        <record id="product_pricelist_import_line_tree_view" model="ir.ui.view">
            <field name="name">product.pricelist.import.line.tree.view</field>
            <field name="model">product.pricelist.import.line</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="File Lines" default_order="fail desc">
                    <field name="code" />
                    <field name="supplier_code" />
                    <field name="supplier_name" />
                    <field name="price"/>
                    <field name="available_qty" />
                    <field name="discount" />
                    <field name="delay" />
                    <field name="fail" />
                    <field name="fail_reason" />
                </tree>
            </field>
        </record>

        <!-- ************* file_load form View  *************** -->
        <record id="product_pricelist_import_line_form_view" model="ir.ui.view">
            <field name="name">product.pricelist.import.line.form.view</field>
            <field name="model">product.pricelist.import.line</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="File Load">
                    <group colspan="4">
                        <field name="code" />
                        <field name="supplier_code" />
                        <field name="supplier_name" />
                        <field name="price"/>
                        <field name="available_qty" />
                        <field name="discount" />
                        <field name="delay" />
                        <field name="fail" />
                        <field name="fail_reason" />
                    </group>
                </form>
            </field>
        </record>
    </data>
</openerp>
